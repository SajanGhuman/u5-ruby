<h1>Products#index</h1>
<p>Find me in app/views/products/index.html.erb</p>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Price</th>
      <th>Stock Quantity</th>
      <th>Category</th>
      <th colspan="5"></th>
    </tr>
  </thead>
  <tbody>
      <tr>
        <td><%= @product.title %></td>
        <td>Description</td>
        <td><%= @product.price %></td>
        <td><%= @product.stock_quantity %></td>
        <td><%= @product.category.name %></td>
        
      </tr>
  </tbody>
</table>